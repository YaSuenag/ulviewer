                    </children>
                </HBox>
                    <TableView fx:id="ageTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                        <columns>
                            <TableColumn fx:id="ageColumn" text="Age" />
                            <TableColumn fx:id="bytesColumn" prefWidth="200.0" text="Bytes" />
                        </columns>
                    </TableView>
            </children>
        </VBox>
