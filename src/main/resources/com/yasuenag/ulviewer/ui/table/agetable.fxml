<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="280.0" prefWidth="320.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yasuenag.ulviewer.ui.table.AgeTableController">
            <children>
                <HBox spacing="20.0">
                    <children>
                        <ComboBox fx:id="logCombo" prefWidth="150.0" />
